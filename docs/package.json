{
  "name": "ember-animated-docs",
  "version": "1.0.4",
  "private": true,
  "description": "Docs app for Ember Animated",
  "repository": "https://github.com/ember-animation/ember-animated",
  "license": "MIT",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "deploy": "ember deploy production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.22.15",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@faker-js/faker": "^6.2.0",
    "@fortawesome/ember-fontawesome": "^0.3.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.1",
    "ember-animated": "1.0.4",
    "ember-animated-tools": "^1.0.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~5.3.0",
    "ember-cli-addon-docs": "file:ember-cli-addon-docs-4.2.2.tgz",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.0.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-component-css": "^0.7.4",
    "ember-data": "~5.3.0",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-moment": "^10.0.0",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-simple-track-helper": "^1.0.0",
    "ember-source": "~5.3.0",
    "ember-template-lint": "^5.11.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^8.0.0",
    "loader.js": "^4.7.0",
    "moment": "^2.29.3",
    "prettier": "^3.0.3",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "rollup": "^3.7.3",
    "sass": "^1.50.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "tracked-built-ins": "^3.2.0",
    "webpack": "^5.88.2"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "volta": {
    "extends": "../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      "../addon"
    ]
  }
}
