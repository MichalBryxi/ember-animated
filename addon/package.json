{
  "name": "ember-animated",
  "version": "1.0.4",
  "description": "Core animation primitives for Ember",
  "keywords": [
    "animation",
    "ember",
    "ember-addon",
    "transition"
  ],
  "homepage": "https://ember-animation.github.io/ember-animated/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-animation/ember-animated.git",
    "directory": "addon"
  },
  "license": "MIT",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4",
    "@embroider/macros": "^1.10.0",
    "assert-never": "^1.2.1",
    "ember-element-helper": "^0.5.0 || ^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.6",
    "@embroider/addon-dev": "^3.0.0",
    "@glimmer/env": "^0.1.7",
    "@tsconfig/ember": "^1.0.0",
    "@types/ember": "^4.0.3",
    "@types/ember__application": "^4.0.4",
    "@types/ember__array": "^4.0.3",
    "@types/ember__component": "^4.0.0",
    "@types/ember__controller": "^4.0.4",
    "@types/ember__debug": "^4.0.3",
    "@types/ember__engine": "^4.0.4",
    "@types/ember__error": "^4.0.2",
    "@types/ember__object": "^4.0.0",
    "@types/ember__owner": "^4.0.2",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.0",
    "@types/ember__runloop": "^4.0.0",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.16.0",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__test-helpers": "^2.6.1",
    "@types/ember__utils": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "concurrently": "^7.6.0",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-source": "*",
    "ember-template-lint": "^5.2.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "rollup": "^3.7.3",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "@ember/test-helpers": "^2.6.0"
  },
  "peerDependenciesMeta": {
    "@ember/test-helpers": {
      "optional": true
    }
  },
  "engines": {
    "node": "12.* || 14.* || 16.* || >= 18"
  },
  "volta": {
    "extends": "../package.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "demoURL": "https://ember-animation.github.io/ember-animated/",
    "main": "addon-main.js",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./components/animated-beacon.js": "./dist/_app_/components/animated-beacon.js",
      "./components/animated-container.js": "./dist/_app_/components/animated-container.js",
      "./components/animated-each.js": "./dist/_app_/components/animated-each.js",
      "./components/animated-if.js": "./dist/_app_/components/animated-if.js",
      "./components/animated-orphans.js": "./dist/_app_/components/animated-orphans.js",
      "./components/animated-value.js": "./dist/_app_/components/animated-value.js",
      "./components/ea-list-element.js": "./dist/_app_/components/ea-list-element.js",
      "./services/-ea-motion.js": "./dist/_app_/services/-ea-motion.js"
    }
  }
}
